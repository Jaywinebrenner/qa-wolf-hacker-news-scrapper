{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
            "buildCommand": "npm install && npx playwright install"

        }
      }
    ],
    "routes": [
      { "src": "/api/articles", "dest": "/index.js" },
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/index.js" }
    ]
  }
  